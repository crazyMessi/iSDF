
{
    "name": "Encoder",
    "model": {
        "from_pretrain":true,
        "pretrain_path": "checkpoints/encoder.pth",

        "SparseConv3dEncoder": {
            "args": {
                "in_channels": 1,
                "latent_channels": 8,
                "num_res_blocks": 2,
                "num_res_blocks_middle": 2,
                "channels": [32, 128, 512],
                "use_fp16": true
            }
        },
        "SparseLatentEncoder": {
            "args": {
                "resolution": 64,
                "in_channels": 1024,
                "model_channels": 768,
                "latent_channels": 8,
                "model2latent": [768,768,512,256,128,64,8],
                "num_blocks": 12,
                "num_heads": 12,
                "mlp_ratio": 4,
                "attn_mode": "swin",
                "window_size": 8,
                "use_fp16": true
            }
        }
    }
}